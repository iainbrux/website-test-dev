op: 20px;\n    display: flex;\n}\n\na {\n    text-decoration: none;\n}\n\na:visited {\n    color: grey;\n}\n\n.tabs .home,\n.tabs .portfolio,\n.tabs .about,\n.tabs .contact {\n    height: 35px;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    color: grey;\n    padding: 5px;\n}\n\n.tabs .home:hover,\n.tabs .portfolio:hover,\n.tabs .about:hover,\n.tabs .contact:hover {\n    transition-duration: 0.5s;\n    cursor: pointer;\n    border-bottom: 1px solid lightgrey;\n    color: lightgrey;\n}\n\n.tabs .active {\n    transition-duration: 0.5s;\n    cursor: pointer;\n    border-bottom: 1px solid lightgrey;\n    color: lightgrey;\n}\n\nmain {\n    background-size: cover;\n    color: rgb(177, 177, 177);\n}\n\n.main-container {\n    padding: 200px 0;\n}\n\n.main-container .home {\n    display: flex;\n    align-items: center;\n    justify-content: center;\n}\n\n.box {\n    margin: 20px 30px;\n    border: 2px solid grey;\n    border-radius: 20px;\n    min-width: 280px;\n    max-width: 330px;\n}\n\n.box h2 {\n    margin: 50px auto;\n    text-align: center;\n    font-size: 35px;\n}\n\n.git-div {\n    display: flex;\n    justify-content: space-around;\n    margin: 40px auto;\n}\n\n.div-image {\n    position: relative;\n}\n\n.div-image::after {\n    content: '';\n    position: absolute;\n    top: 0;\n    bottom: 0;\n    right: 0;\n    left: 0;\n    background-image: linear-gradient(to bottom, rgba(0,0,0,0.5), rgba(0,0,0,8));\n    z-index: 0;\n}\n\n.git-src,\n.git-live {\n    border: 1px solid grey;\n    padding: 20px;\n    border-radius: 15px;\n    font-weight: bolder;\n    word-wrap: normal;\n}\n\n.git-src {\n    background-color: #111111;\n    transition-duration: 0.5s;\n}\n\n.git-src:hover {\n    background-color: #00ca8e;\n    color: black;\n}\n\n.git-live {\n    background-image: linear-gradient(to top right, #00ca8e, rgb(47, 224, 255));\n    color: black;\n}\n\n.main-container {\n    position: absolute;\n    height: 100%;\n    width: 100%;\n}\n\n